{
    "name": "netresearch/sdk-api-central-station",
    "type": "library",
    "description": "Implementation of the Central Station API",
    "license": "MIT",
    "authors": [
        {
            "name": "Rico Sonntag",
            "email": "rico.sonntag@netresearch.de",
            "role": "Developer",
            "homepage": "https://www.netresearch.de/"
        }
    ],
    "support": {
        "issues": "https://git.netresearch.de/futuresax/lib/sdk-api-central-station"
    },
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "psr/http-client-implementation": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-factory-implementation": "^1.0",
        "psr/http-message": "^1.0",
        "psr/http-message-implementation": "^1.0",
        "psr/log": "^1.1",
        "php-http/discovery": "^1.14",
        "php-http/httplug": "^2.3",
        "php-http/logger-plugin": "^1.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "magicsunday/jsonmapper": "^2.0"
    },
    "require-dev": {
        "php-http/mock-client": "dev-upgrade-version-2",
        "php-http/guzzle6-adapter": "^2.0",
        "nyholm/psr7": "^1.5",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Netresearch\\Sdk\\CentralStation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netresearch\\Sdk\\CentralStation\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
