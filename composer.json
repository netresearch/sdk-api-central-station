{
    "name": "netresearch/sdk-api-central-station",
    "type": "library",
    "description": "Implementation of the Central Station API",
    "license": "MIT",
    "authors": [
        {
            "name": "Rico Sonntag",
            "email": "rico.sonntag@netresearch.de",
            "role": "Developer",
            "homepage": "https://www.netresearch.de/"
        }
    ],
    "support": {
        "issues": "https://git.netresearch.de/futuresax/lib/sdk-api-central-station"
    },
    "require": {
        "php": "~8.1.0 || ~8.2.0",
        "ext-json": "*",
        "psr/http-factory-implementation": "*",
        "psr/http-client-implementation": "*",
        "psr/http-message-implementation": "*",
        "psr/http-factory": "^1.0.0",
        "psr/http-client": "^1.0.0",
        "psr/http-message": "^1.0.0",
        "psr/log": "^1.0.0",
        "php-http/discovery": "^1.18.0",
        "php-http/httplug": "^2.4.0",
        "php-http/message": "^1.15.0",
        "php-http/logger-plugin": "^1.3.0",
        "phpdocumentor/reflection-docblock": "^5.3.0",
        "magicsunday/jsonmapper": "^2.2.0"
    },
    "require-dev": {
        "php-http/mock-client": "^1.5",
        "php-http/message-factory": "^1.1",
        "symfony/http-client": "^6.2",
        "nyholm/psr7": "^1.8",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.7",
        "rector/rector": "^0.16"
    },
    "autoload": {
        "psr-4": {
            "Netresearch\\Sdk\\CentralStation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Netresearch\\Sdk\\CentralStation\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
